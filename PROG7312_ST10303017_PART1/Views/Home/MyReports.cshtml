@model PROG7312_ST10303017_PART1.DataStructures.CustomLinkedList<PROG7312_ST10303017_PART1.Models.ServiceRequest>
@{
    ViewData["Title"] = "My Service Reports";
}

<h1>My Reports</h1>
<p>Here is a list of all the service requests you have submitted.</p>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Tracking ID</th>
            <th>Category</th>
            <th>Status</th>
            <th>Date Submitted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TrackingId</td>
                <td>@item.Category</td>
                <td>@item.Status</td>
                <td>@item.SubmittedDate.ToString("yyyy-MM-dd HH:mm")</td>
            </tr>
        }
    </tbody>
</table>